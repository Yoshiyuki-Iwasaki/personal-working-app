{
  "functions": {
    "source": "./functions/",
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build",
    "runtime": "nodejs12"
  },
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "trailingSlash": true
  }
}
